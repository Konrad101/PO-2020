        <div class="content_big_tab" style="left: 410px; top: 404px;">
            <label>Lista ocen</label>
            <table>
                <thead>
                <td class="col_1">#</td>
                <td class="col_2">Data</td>
                <td class="col_3">Ocena</td>
                <td class="col_4">Komentarz</td>
                <td class="col_5"></td>
                <td class="col_6"></td>
                </thead>

                @foreach (var item in ViewBag.finalList)
                {
                    <tr>
                        <td class="col_1">@(ViewBag.finalList.IndexOf(item)+1)</td>
                        <td class="col_2">@item.Item1.ToString("dd.MM.yyyy")</td>
                        <td class="col_3">@GradeConverter.ParseGrade(item.Item2)</td>
                        <td class="col_4">@item.Item3</td>
                        <td class="col_5">
                            <a asp-controller="LecturerGradesList" asp-action="Edit" asp-route-id="@item.Item4.PartialGradeId">
                                <img class="img " src="/../images/lecturerfinalthesislist/u131.png" />
                            </a>
                        </td>
                        <td class="col_6">
                            <a asp-controller="LecturerGradesList" asp-action="Delete" asp-route-id="@item.Item4.PartialGradeId">
                                <img class="img " src="/../images/lecturerfinalthesislist/u132.png" />
                            </a>
                        </td>
                    </tr>
                }

