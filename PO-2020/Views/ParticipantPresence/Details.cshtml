        @using (Html.BeginForm("Index", "ParticipantPresence"))
        {
            <!-- Wybierz kurs (Group) -->
            <div id="u591" class="ax_default" data-label="Wybierz kurs" data-left="410" data-top="148" data-width="1480" data-height="172">

