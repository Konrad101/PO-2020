        }


            <div class="content_small_tab">
                <label>Lista obecności</label>
                <table>
                    <thead>
                    <td class="col_1">#</td>
                    <td class="col_2">Data</td>
                    <td class="col_3">Start</td>
                    <td class="col_4">Koniec</td>
                    <td class="col_5">Obecność</td>
                    </thead>
                     
                    @foreach (var item in ViewBag.classesUnits)
                    {

                        <tr class="@if (ViewBag.attendancesBool[ViewBag.classesUnits.IndexOf(item)])
                        {
                            @("present")
                        }
                        else
                        {
                            @("absent")
                        }">
                            <td class="col_1">@(ViewBag.classesUnits.IndexOf(item)+1)</td>
                            <td class="col_2">@item.ClassBeginning.ToString("dd.MM.yyyy")</td>
                            <td class="col_3">@item.ClassBeginning.ToString("HH:mm")</td>
                            <td class="col_4">@item.ClassEnding.ToString("HH:mm")</td>
                            <td class="col_5">@if (ViewBag.attendancesBool[ViewBag.classesUnits.IndexOf(item)])
                            {
                                @("Obecny")
                            }
                            else
                            {
                                @("Nieobecny")
                            }</td>
                        </tr>
                    }
                </table>
                @if (!ViewBag.attendancesAvailable)
                {
                    <h3>Brak danych o obecnościach</h3>
                }
            </div>
            <!-- Statystyki (Group) -->
            <div id="u616" class="ax_default" data-label="Statystyki" data-left="1165" data-top="350" data-width="725" data-height="208">

